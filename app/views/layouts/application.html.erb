<!DOCTYPE html>
<html>
  <head>
    <title>Speedtest2</title>
    <meta charset="utf-8">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 
                      'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!--[if lt IE 9]>
      <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/r29/html5.min.js">
      </script>
    <![endif]-->
  </head>

  <body>
    <div id="container">

      <div id="header">
        <h1>Test the speed of your computer!</h1>
      </div>

      <div class = "nav">
        <ul class="nav">
          <li><%= link_to "Home", root_path %></li>
          <li><%= link_to "Test Speed", test_path %></li>
          <li><%= link_to "Results", results_path %></li>
          <li><%= link_to "Languages Compared", languages_path %></li>
          <li> <%= link_to "Browsers Compared", browsers_path %></li>
       </ul>
      </div>

      <div id = "main">
        <%= yield %>
      </div>

      <div id ="footer">
      </div>

    </div>
  </body>
</html>

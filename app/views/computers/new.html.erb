<%= form_for(@computer) do |f| %>
  <%= render 'computers/errors' %>

  <p>
    <%= f.label :userName, "Your name (required)" %><br>
    <%= f.text_field :userName %>
  </p>

  <p>
    <%= f.label :browser, "Browser name" %><br>
    <%= f.select( :browser, options_for_select([
["Other", "Unknown browser"], ["Internet Explorer 9", "Internet Explorer 9"], ["Internet Explorer 8", "Internet Explorer 8"], ["Internet Explorer 7","Internet Explorer 7"], ["Internet Explorer 6", "Internet Explorer 6"], ["Firefox 3.x", "Firefox 3.x"], ["Firefox 4 or higher", "Firefox 4 or higher"], ["Chrome", "Chrome"], ["Safari", "Safari"], ["Opera", "Opera"]
]))%>
  </p>

  <p>
    <%= f.label :category, "Machine type" %><br>
    <%= f.select( :category, options_for_select([
["Desktop", "Desktop"], ["Laptop", "Laptop"], ["Tablet", "Tablet"], ["Phone", "Phone"], ["Ipod", "Ipod"], ["Other", "Other"]
]))%>
  </p>

  <p>
    <%= f.label :description, "Machine description (126 characters max)"%><br>
    <%= f.text_area :description %>
  </p>
  
  <%= f.hidden_field :time %>
  <%= f.hidden_field :userAgentString %>                          

  <p>
    <%= f.submit "Submit" %>
  </p>

<% end %>

